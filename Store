import java.util.Vector;


public class Store {

	static String Name;
	static String StoreType;
	static String address;
	static int StoreID;
	public static Vector<Product> product = new Vector<Product>();
	
	
	public static void GetStoreInfo()
	{
		
		System.out.println("Name : "+Name);
		System.out.println("Store Type : "+StoreType);
		System.out.println("Store Address : "+address);
		System.out.println("Store ID : "+StoreID);
		
		System.out.println("Store Product's : ");
		
		for(int i=0 ; i <product.size();i++)
		{
			System.out.println("Product Name : "+product.get(i).PName);
			System.out.println("Product Price : "+product.get(i).Price);
			System.out.println("Product Brand Name : "+product.get(i).BrandName);
			System.out.println("Product  Tpye : "+product.get(i).ProductType);
			
			
		}
	}
	

	
	
}
